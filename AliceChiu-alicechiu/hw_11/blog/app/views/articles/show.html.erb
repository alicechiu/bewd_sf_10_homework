<div class="col-xs-12">
  <div class="row-fluid">
    <h1><%= @article.title %></h1>
    <ul><%= @article.date %></ul>
    <ul><%= @article.body %></ul>
    <ul><%= @article.author %></ul>
  </div>
  <div class="row-fluid">
    <%= link_to 'Back', articles_path, class: 'btn btn-lg btn-warning' %>
    <%= link_to 'Edit', edit_article_path(@article), class: 'btn btn-lg btn-warning' %>
  </div>
</div>
